<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    style="@style/MyBackgroundHelp"
    tools:context=".views.helpmenu.SymbolsDefinitionsActivity">


    <TextView
        android:id="@+id/symbols_title"
        style="@style/MyDefaultText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center"
        android:text="Symbols"
        android:textSize="28sp"
        android:typeface="sans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/input_voltage_symbol"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center|start"
        android:text="Vi"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        app:flow_horizontalBias="0"
        app:layout_constraintEnd_toStartOf="@+id/input_voltage_text_symbol"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/symbols_title" />

    <TextView
        android:id="@+id/input_voltage_text_symbol"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="start"
        android:text="Input Voltage"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/symbols_title"
        app:layout_constraintTop_toBottomOf="@+id/symbols_title" />

    <TextView
        android:id="@+id/inductor_text_symbol"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="start"
        android:text="Inductor"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/symbols_title"
        app:layout_constraintTop_toBottomOf="@+id/input_voltage_text_symbol" />

    <TextView
        android:id="@+id/inductance_symbol"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center|start"
        android:text="L"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        app:flow_horizontalBias="0"
        app:layout_constraintEnd_toStartOf="@+id/inductor_text_symbol"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/input_voltage_symbol" />

    <TextView
        android:id="@+id/switch_text_symbol"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="start"
        android:text="Switch"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/symbols_title"
        app:layout_constraintTop_toBottomOf="@+id/inductor_text_symbol" />

    <TextView
        android:id="@+id/switch_symbol"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center|start"
        android:text="S"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        app:flow_horizontalBias="0"
        app:layout_constraintEnd_toStartOf="@+id/switch_text_symbol"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inductance_symbol" />

    <TextView
        android:id="@+id/diode_text_symbol"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="start"
        android:text="Diode"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/symbols_title"
        app:layout_constraintTop_toBottomOf="@+id/switch_text_symbol" />

    <TextView
        android:id="@+id/diode_symbol"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center|start"
        android:text="D"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        app:flow_horizontalBias="0"
        app:layout_constraintEnd_toStartOf="@+id/diode_text_symbol"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch_symbol" />

    <TextView
        android:id="@+id/capacitance_symbol"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center|start"
        android:text="C"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        app:flow_horizontalBias="0"
        app:layout_constraintEnd_toStartOf="@+id/capacitor_text_symbol"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/diode_symbol" />

    <TextView
        android:id="@+id/capacitor_text_symbol"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="start"
        android:text="Capacitor"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/symbols_title"
        app:layout_constraintTop_toBottomOf="@+id/diode_text_symbol" />

    <TextView
        android:id="@+id/resistor_text_symbol"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="start"
        android:text="Resistor (Load)"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/symbols_title"
        app:layout_constraintTop_toBottomOf="@+id/capacitor_text_symbol" />

    <TextView
        android:id="@+id/resistance_symbol"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center|start"
        android:text="R"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        app:flow_horizontalBias="0"
        app:layout_constraintEnd_toStartOf="@+id/resistor_text_symbol"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/capacitance_symbol" />

    <TextView
        android:id="@+id/snubber_capacitor_text_symbol"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="start"
        android:text="Snubber Capacitor"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/symbols_title"
        app:layout_constraintTop_toBottomOf="@+id/resistor_text_symbol" />

    <TextView
        android:id="@+id/snubber_capacitance_symbol"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center|start"
        android:text="Cs"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        app:flow_horizontalBias="0"
        app:layout_constraintEnd_toStartOf="@+id/snubber_capacitor_text_symbol"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resistance_symbol" />

    <TextView
        android:id="@+id/resistance_snubber_text_symbol"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center|start"
        android:text="Rs"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        app:flow_horizontalBias="0"
        app:layout_constraintEnd_toStartOf="@+id/snubber_resistor_text_symbol"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/snubber_capacitance_symbol" />

    <TextView
        android:id="@+id/snubber_resistor_text_symbol"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="start"
        android:text="Snubber Resistor"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/symbols_title"
        app:layout_constraintTop_toBottomOf="@+id/snubber_capacitor_text_symbol" />

    <TextView
        android:id="@+id/snubber_diode_text_symbol"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="start"
        android:text="Snubber Diode"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/symbols_title"
        app:layout_constraintTop_toBottomOf="@+id/snubber_resistor_text_symbol" />

    <TextView
        android:id="@+id/diode_snubber_text_symbol"
        android:layout_width="32dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="center|start"
        android:text="Ds"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        app:flow_horizontalBias="0"
        app:layout_constraintEnd_toStartOf="@+id/snubber_diode_text_symbol"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resistance_snubber_text_symbol" />
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>